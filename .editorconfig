# EditorConfig auxilia a manter o estilo de código em quanto estamos trabalhando, porém não corrige códigos existentes.

# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
indent_style = space
indent_size = 2
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = false
insert_final_newline = false
